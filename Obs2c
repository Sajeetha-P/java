package Sajee;
class Account {
    int accno;
    double balance;

    // Default constructor
    Account() {
        accno = 0;
        balance = 0.0;
    }

    // Constructor with one parameter
    Account(int an) {
        accno = an;
        balance = 0.0;
    }

    // Constructor with two parameters
    Account(int an, double amt) {
        accno = an;
        balance = amt;
    }

    // Method with one argument
    void read(int an) {
        accno = an;
        balance = 0.0;
    }

    // Method with two arguments
    void read(int an, double amt) {
        accno = an;
        balance = amt;
    }

    // Method to print account details
    void print() {
        System.out.println("Account No: " + accno + ", Balance: " + balance);
    }
}

public class Obv2c {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Account a1 = new Account();
        Account a2 = new Account(101);
        Account a3 = new Account(102, 5000.50);

        // Objects created using default constructor then initialized with methods
        Account a4 = new Account();
        a4.read(2);  // Calling method with one argument

        Account a5 = new Account();
        a5.read(3, 600);  // Calling method with two arguments

        // Printing details
        a1.print();
        a2.print();
        a3.print();
        a4.print();
        a5.print();
	}

}
